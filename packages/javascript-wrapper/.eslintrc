{
    "extends": ["@gravity-ui/eslint-config", "@gravity-ui/eslint-config/prettier"],
    "root": true,
    "rules": {
        "new-cap": 0,
        "import/order": 0,
        "import/dynamic-import-chunkname": [2, {
            "importFunctions": ["dynamicImport"],
            "webpackChunknameFormat": "[a-zA-Z-]+",
            "allowEmpty": false
        }],
    },
    "env": {
        "jest": true,
        "node": true,
        "browser": true
    }
}
