{
  "name": "@ytsaurus/ui",
  "version": "0.0.2",
  "private": true,
  "repository": {
    "type": "github",
    "url": "https://github.com/YTsaurus/ytsaurus-ui/tree/main/packages/ui"
  },
  "scripts": {
    "prepublishOnly": "npm run build:lib",
    "build": "npm run copy:icons &&  NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production app-builder build --config ./build.app.config.ts",
    "build:lib": "rm -rf build && npm run build:lib:ui && npm run build:lib:server",
    "build:lib:ui": "NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production app-builder build --config ./build.lib.config.ts && cp -r img assets build",
    "build:lib:server": "tsc -p ./build.server.tsconfig.json && which tsc",
    "clean": "rm -rf dist/*",
    "copy:icons": "mkdir -p dist/public && cp img/favicon.png dist/public",
    "cut:version": "node -e \"const json=require('${npm_config_file}'); delete json.version; console.log(JSON.stringify(json, null, 2))\"",
    "debug": "npm run copy:icons && NODE_OPTIONS=\"--max-http-header-size=204800 ${NODE_OPTIONS}\" app-builder dev --inspect --config ./build.app.config.ts",
    "deps:build": "npm run build",
    "deps:install": "npm ci",
    "deps:truncate": "npm prune --production",
    "dev": "test -f dist/.tvm.json && npm run dev:app",
    "dev:oss": "APP_INSTALLATION=oss npm run dev",
    "dev:app": "./scripts/check-start-files.sh && npm run copy:icons && APP_ENV=${APP_ENV:-development} APP_DEV_MODE=1 NODE_OPTIONS=\"--max-http-header-size=204800 ${NODE_OPTIONS}\" app-builder dev --config ./build.app.config.ts",
    "dev:localmode": "bash -c '. scripts/dev.localmode-env.sh && TVM_DISABLED=true npm run dev:app'",
    "docker:build": "docker build . -t ${npm_package_docker_image}:${npm_config_dockertag}",
    "docker:push": "docker push ${npm_package_docker_image}:${npm_config_dockertag}",
    "lint": "npm run lint:file-names && npm run lint:js && npm run lint:styles",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:styles -- --fix",
    "lint:js": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --ignore-pattern src/ui/vendor --quiet",
    "lint:styles": "stylelint 'src/ui/**/*.scss' --customSyntax postcss-scss",
    "lint:file-names": "./scripts/check-file-names-for-js-duplicates.sh",
    "show:file-types": "find src -type f | rev | cut -d '.' -f 1 | rev | sort | uniq -c | sort -rn",
    "notify:mail": "node scripts/notify.js",
    "platform-tools": "platform-tools",
    "show:docker-image-name": "echo ${npm_package_docker_image}",
    "show:version": "echo ${npm_package_version}",
    "start": "NODE_OPTIONS=\"--max-http-header-size=204800 ${NODE_OPTIONS}\" node dist/server",
    "test": "npm run test:unit",
    "test:coverage": "APP_ENV=local jest --config ./jest.config.js --coverage",
    "test:lint": "npm run lint",
    "test:e2e": "cd tests; npm ci; npm run test:e2e",
    "test:e2e:dev": "E2E_MATCH=e2e1 npm run test:e2e:show",
    "test:e2e:localmode": "cd tests; npm ci; cd ..; npm run test:e2e:localmode:noci",
    "test:e2e:localmode:dev": "export HEADLESS=false; E2E_MATCH=local-e2e1 npm run test:e2e:localmode:noci",
    "test:e2e:localmode:noci": "cd tests; export PLAYWRIGHTER_YT_CLUSTER=ui; export PLAYWRIGHTER_SKIP_AUTH=true; E2E_MATCH=${E2E_MATCH:-local-e2e} bash -c 'E2E_DIR=//home npm run test:e2e'",
    "test:e2e:noci": "cd tests; npm run test:e2e",
    "test:e2e:show": "HEADLESS=false npm run test:e2e:noci",
    "test:unit": "jest --config ./jest.config.js",
    "test:smoke": "cd smoke-tests; npm ci; npm test",
    "test:smoke:noci": "cd smoke-tests; npm test",
    "test:watch": "jest --config ./jest.config.js --watchAll"
  },
  "dependencies": {
    "@gravity-ui/app-layout": "^1.1.1",
    "@gravity-ui/dialog-fields": "^1.0.1",
    "@gravity-ui/expresskit": "^0.1.2",
    "@gravity-ui/navigation": "^0.3.0",
    "@gravity-ui/nodekit": "^0.1.0",
    "@gravity-ui/unipika": "^1.0.0",
    "@ytsaurus/interface-helpers": "^0.1.1",
    "@ytsaurus/javascript-wrapper": "^0.2.1",
    "axios": "^0.21.4",
    "cookie-parser": "^1.4.6",
    "highlight.js": "^10.7.3",
    "js-cookie": "~2.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "object-hash": "^2.2.0",
    "qs": "^6.11.0",
    "source-map-support": "^0.5.21",
    "type-is": "^1.6.18"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@doc-tools/transform": "^2.9.0",
    "@gravity-ui/app-builder": "^0.0.1",
    "@gravity-ui/chartkit": "^1.4.0",
    "@gravity-ui/dashkit": "^3.0.2",
    "@gravity-ui/i18n": "^1.0.0",
    "@gravity-ui/icons": "^1.1.0",
    "@gravity-ui/uikit": "^3.18.0",
    "@types/d3": "^7.4.0",
    "@types/enzyme": "^3.10.12",
    "@types/express": "^4.17.13",
    "@types/fontfaceobserver": "^2.1.0",
    "@types/jest": "^26.0.24",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.182",
    "@types/object-hash": "^2.2.1",
    "@types/qs": "^6.9.7",
    "@types/react-dom": "^17.0.18",
    "@types/react-helmet": "^6.1.5",
    "@types/react-list": "^0.8.7",
    "@types/react-redux": "^7.1.24",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-sticky": "^6.0.4",
    "@types/reselect": "^2.2.0",
    "@types/type-is": "^1.6.3",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "@yandex-cloud/eslint-config": "^1.1.0",
    "@yandex-cloud/prettier-config": "^1.0.0",
    "@yandex-cloud/react-data-table": "^1.0.2",
    "@yandex-cloud/stylelint-config": "^1.2.0",
    "@yandex-cloud/tsconfig": "^1.0.0",
    "bem-cn-lite": "~4.0.0",
    "bignumber.js": "^9.0.2",
    "copy-to-clipboard": "^3.3.1",
    "d3": "^7.5.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fontfaceobserver": "^2.1.0",
    "history": "~4.10.1",
    "hoist-non-react-statics": "~3.3.2",
    "hotkeys-js": "^3.9.4",
    "identity-obj-proxy": "^3.0.0",
    "immer": "^9.0.15",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-svg-transformer": "^1.0.0",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "luxon": "^1.28.0",
    "lz-string": "~1.4.4",
    "monaco-editor": "^0.31.1",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "opentracing": "^0.14.7",
    "postcss": "^8.4.14",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "prop-types": "~15.7.2",
    "rc-slider": "^8.7.1",
    "react": "^17.0.0",
    "react-datetime": "~2.16.3",
    "react-dom": "^17.0.0",
    "react-helmet": "~6.0.0",
    "react-hot-loader": "~4.12.20",
    "react-list": "^0.8.17",
    "react-redux": "^7.2.8",
    "react-router": "^5.3.3",
    "react-router-dom": "^5.3.3",
    "react-sortable-hoc": "^2.0.0",
    "react-split-pane": "^0.1.92",
    "react-sticky": "~6.0.3",
    "redux": "~4.0.5",
    "redux-location-state": "~2.8.2",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "~2.3.0",
    "reselect": "~4.0.0",
    "resize-observer-polyfill": "~1.5.1",
    "sass": "^1.56.1",
    "screenfull": "~5.0.2",
    "split.js": "~1.5.7",
    "stylelint": "^14.9.1",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.9.3",
    "vis-network": "^9.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "docker": {
    "image": "ytsaurus/ui"
  }
}
