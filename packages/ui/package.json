{
  "name": "@ytsaurus/ui",
  "version": "0.23.1",
  "repository": {
    "type": "github",
    "url": "https://github.com/YTsaurus/ytsaurus-ui/tree/main/packages/ui"
  },
  "files": [
    "build",
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build:lib && npm run build",
    "build": "npm run copy:icons &&  NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production app-builder build --config ./build.app.config.ts",
    "build:lib": "rm -rf build && npm run build:lib:ui && npm run build:lib:server",
    "build:lib:ui": "NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production app-builder build --config ./build.lib.config.ts && cp -r img assets build",
    "build:lib:server": "tsc -p ./build.server.tsconfig.json",
    "clean": "rm -rf dist/*",
    "copy:icons": "mkdir -p dist/public && cp img/favicon.png dist/public",
    "cut:version": "node -e \"const json=require('${npm_config_file}'); delete json.version; console.log(JSON.stringify(json, null, 2))\"",
    "debug": "npm run copy:icons && NODE_OPTIONS=\"--max-http-header-size=204800 ${NODE_OPTIONS}\" app-builder dev --inspect --config ./build.app.config.ts",
    "deps:build": "npm run build",
    "deps:install": "npm ci --legacy-peer-deps",
    "deps:truncate": "npm prune --omit=dev --legacy-peer-deps",
    "dev": "npm run dev:app",
    "dev:app": "./scripts/check-start-files.sh && npm run copy:icons && APP_ENV=${APP_ENV:-development} APP_DEV_MODE=1 NODE_OPTIONS=\"--max-http-header-size=204800 ${NODE_OPTIONS}\" app-builder dev --config ./build.app.config.ts",
    "dev:localmode": "bash -c '. scripts/dev.localmode-env.sh && TVM_DISABLED=true npm run dev:app'",
    "dev:localmode:e2e": "APP_INSTALLATION=${APP_INSTALLATION:-e2e} npm run dev:localmode",
    "dev:oss": "APP_INSTALLATION=oss npm run dev",
    "docker:build": "touch ytsaurus-ui-empty.tgz; docker build ${BUILD_ARGS} --build-arg UI_VERSION=$(npm run show:version | tail -1) . -t ${npm_package_config_docker_image}:${npm_config_dockertag}",
    "docker:build:dev": "npm run build && npm pack && BUILD_ARGS=\"--build-arg DEV=1\" npm run docker:build",
    "docker:push": "docker push ${npm_package_config_docker_image}:${npm_config_dockertag}",
    "lint": "npm run lint:file-names && npm run lint:js && npm run lint:styles",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:styles -- --fix",
    "lint:js": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --ignore-pattern src/ui/vendor --quiet",
    "lint:styles": "stylelint 'src/ui/**/*.scss' --customSyntax postcss-scss",
    "lint:file-names": "./scripts/check-file-names-for-js-duplicates.sh",
    "show:file-types": "find src -type f | rev | cut -d '.' -f 1 | rev | sort | uniq -c | sort -rn",
    "notify:mail": "node scripts/notify.js",
    "platform-tools": "platform-tools",
    "show:docker-image-name": "echo ${npm_package_config_docker_image}",
    "show:version": "echo ${npm_package_version}",
    "start": "NODE_OPTIONS=\"--max-http-header-size=204800 ${NODE_OPTIONS}\" node dist/server",
    "test": "npm run test:unit",
    "e2e:ci": "cd tests; npm ci",
    "e2e:localmode:init": "YT_PROXY=${YT_PROXY:-$(hostname):8000} tests/init-cluster-e2e.sh",
    "e2e:localmode:ui": "CLUSTER=ui CLUSTER_TITLE=Local npm run e2e:start",
    "e2e:localmode:remote": "CLUSTER=`hostname`:8000 CLUSTER_TITLE='Local as remote' npm run e2e:start",
    "e2e:start": "npm run e2e:notice; cd tests; echo $(cat ../e2e-env.tmp) BASE_URL=${BASE_URL:-http://$(hostname):8001} npm start | bash",
    "e2e:notice": "echo \"\\n\\n!!!!!! Do not forget to run 'npm run e2e:localmode:init' !!!!!!\\n\\n\"",
    "test:unit": "jest --config ./jest.config.js",
    "test:smoke": "cd smoke-tests; npm ci; npm test",
    "test:smoke:noci": "cd smoke-tests; npm test",
    "test:watch": "jest --config ./jest.config.js --watchAll"
  },
  "dependencies": {
    "@doc-tools/transform": "3.3.2",
    "@gravity-ui/app-layout": "1.1.1",
    "@gravity-ui/expresskit": "0.2.0",
