    "@types/rbush": "^3.0.0",
    "@ytsaurus/interface-helpers": "^0.3.0",
    "@ytsaurus/javascript-wrapper": "^0.7.1",
    "axios": "^1.5.0",
    "colord": "^2.9.1",
    "cookie-parser": "1.4.6",
    "d3-scale": "4",
    "d3-selection": "3",
    "d3-shape": "3",
